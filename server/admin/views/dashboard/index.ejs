<% include ../partials/header %>

      <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
        <h1 class="page-header">Dashboard</h1>

        <%if (typeof message !== 'undefined' && message.length > 0) { %>
          <p>
            <strong></strong><%= message %></strong>
          </p>
        <% } %>

        <div class="row placeholders">
          <div class="col-xs-4 placeholder">
            <h4><%= events.length %></h4>
            <span class="text-muted">Events</span>
          </div>
          <div class="col-xs-4 placeholder">
            <h4><%= organisations.length %></h4>
            <span class="text-muted"><a href="/admin/organisations">Organisers</a></span>
          </div>
          <div class="col-xs-4 placeholder">
            <h4><%= venues.length %></h4>
            <span class="text-muted">Venues</span>
          </div>
        </div>

        <div class="quick-actions">
          <h2>Quick Actions</h2>
          <a href="/admin/organisation/new" class="btn btn-success">New Organisation</a>
        </div>

      </div>

<% include ../partials/footer %>
